{
  "name": "danielz/music-codes",
  "description": "Music Codes PHP library provides handy tools for codes in the music industry",
  "keywords": ["isrc", "music", "code"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Zahariev",
      "email": "daniel.zahariev@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "DanielZ\\MusicCodes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DanielZ\\MusicCodes\\Tests\\": "tests/"
    }
  }
}
